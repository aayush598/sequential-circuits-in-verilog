    %pad/u 5;
    %load/vec4 v0x5c0e92903e00_0;
    %pad/u 5;
    %add;
    %split/vec4 4;
    %store/vec4 v0x5c0e92903c30_0, 0, 4;
    %store/vec4 v0x5c0e92903ee0_0, 0, 1;
    %jmp T_0.9;
T_0.1 ;
