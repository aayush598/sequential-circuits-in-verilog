    %store/vec4 v0x5c0e92903c30_0, 0, 4;
    %store/vec4 v0x5c0e92903ee0_0, 0, 1;
    %jmp T_0.9;
T_0.5 ;
    %pushi/vec4 0, 0, 1;
    %load/vec4 v0x5c0e928ee280_0;
    %inv;
    %concat/vec4; draw_concat_vec4
    %split/vec4 4;
    %store/vec4 v0x5c0e92903c30_0, 0, 4;
    %store/vec4 v0x5c0e92903ee0_0, 0, 1;
